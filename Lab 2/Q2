def multiplication_table(n):

    table = []


    for i in range(1, n + 1):

        row = [i * j for j in range(1, i + 1)]


        table.append(row)


    return table


n =int( input("Enter the string to search: "))

table = multiplication_table(n)

print(f"The multiplication table from 1 to '{n}' is : {table}")

